@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#>.
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<http://radig.com/plugins/looper1>
	a lv2:Plugin, lv2:UtilityPlugin;
	lv2:project <http://lv2plug.in/ns/lv2>;
	doap:name "Loopor";
	doap:license <http://opensource.org/licenses/isc>;
	lv2:port 
		[
			a lv2:AudioPort, lv2:InputPort;
			lv2:index 0;
			lv2:symbol "in1";
			lv2:name "In1"
		], 
		[
			a lv2:AudioPort, lv2:InputPort;
			lv2:index 1;
			lv2:symbol "in2";
			lv2:name "In2"
	    ], 
	    [
	        a lv2:AudioPort, lv2:OutputPort;
	        lv2:index 2;
	        lv2:symbol "out1";
	        lv2:name "Out1"
		], 
		[
			a lv2:AudioPort, lv2:OutputPort;
			lv2:index 3;
			lv2:symbol "out2";
			lv2:name "Out2"
		],
		[
			a lv2:InputPort, lv2:ControlPort;
			lv2:index 4;
			lv2:symbol "threshold";
			lv2:name "Threshold";
			lv2:default -70.0;
			lv2:minimum -90.0;
			lv2:maximum 24.0;
			units:unit units:db;
		],
		[
		  a lv2:ControlPort, lv2:InputPort;
		  lv2:index 5;
		  lv2:symbol "activate";
		  lv2:name "Activate";
		  lv2:default 0;
		  lv2:minimum 0;
		  lv2:maximum 1;
		  lv2:portProperty lv2:integer, lv2:toggled, pprops:trigger;
		],
		[
		  a lv2:ControlPort, lv2:InputPort;
		  lv2:index 6;
		  lv2:symbol "reset";
		  lv2:name "Reset";
		  lv2:default 0;
		  lv2:minimum 0;
		  lv2:maximum 1;
		  lv2:portProperty lv2:integer, lv2:toggled, pprops:trigger;
		],
		[
		  a lv2:ControlPort, lv2:InputPort;
		  lv2:index 7;
		  lv2:symbol "undo";
		  lv2:name "Undo";
		  lv2:default 0;
		  lv2:minimum 0;
		  lv2:maximum 1;
		  lv2:portProperty lv2:integer, lv2:toggled, pprops:trigger;
		],
		[
		  a lv2:ControlPort, lv2:InputPort;
		  lv2:index 8;
		  lv2:symbol "redo";
		  lv2:name "Redo";
		  lv2:default 0;
		  lv2:minimum 0;
		  lv2:maximum 1;
		  lv2:portProperty lv2:integer, lv2:toggled, pprops:trigger;
		],
		[
		  a lv2:ControlPort, lv2:InputPort;
		  lv2:index 9;
		  lv2:symbol "dub";
		  lv2:name "Dub";
		  lv2:default 0;
		  lv2:minimum 0;
		  lv2:maximum 1;
		  lv2:portProperty lv2:integer, lv2:toggled, pprops:trigger;
		] .	
